  <artifactId>OnlineObjects</artifactId>
  <packaging>war</packaging>
  <version>1.0</version>
  <name>OnlineObjects</name>
  <url>http://www.onlineobjects.com</url>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
  </properties>

  <profiles>
      <profile>
          <id>EssentialTests</id>
          <properties>
              <testcase.groups>dk.in2isoft.onlineobjects.test.EssentialTests</testcase.groups>
          </properties>
      </profile>
  </profiles>

  <build>
    <finalName>OnlineObjects</finalName>
    <plugins>
      <!-- <plugin> <groupId>org.mortbay.jetty</groupId> <artifactId>maven-jetty-plugin</artifactId>
        <version>6.1.26</version> <configuration> <contextPath>/</contextPath> </configuration>
        </plugin> -->
      <plugin>
        <groupId>org.eclipse.jetty</groupId>
        <artifactId>jetty-maven-plugin</artifactId>
        <version>9.3.28.v20191105</version>
        <configuration>
          <contextPath>/</contextPath>
          <webApp>
            <defaultsDescriptor>src/main/webapp/WEB-INF/jetty-defaults.xml</defaultsDescriptor>
          </webApp>
          <jettyXml>src/main/webapp/WEB-INF/jetty.xml</jettyXml>
        </configuration>
      </plugin>

      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.2</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <fork>true</fork>
          <meminitial>1024m</meminitial>
          <maxmem>2024m</maxmem>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.13</version>
        <dependencies>
          <dependency>
            <groupId>org.apache.maven.surefire</groupId>
            <artifactId>surefire-junit47</artifactId>
            <version>2.13</version>
          </dependency>
        </dependencies>
        <configuration>
          <groups>${testcase.groups}</groups>
        </configuration>
      </plugin>
<!--
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jdeps-plugin</artifactId>
        <version>3.0.0</version>
